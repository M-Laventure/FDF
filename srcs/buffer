void	put_menu(t_env *fdf)
{
	mlx_string_put(fdf->mlx_ptr, fdf->win_ptr, -100, 0, 0xffd700, ARROWS);
	mlx_string_put(fdf->mlx_ptr, fdf->win_ptr, -100, 10, 0xffd700, ZOOM);
	mlx_string_put(fdf->mlx_ptr, fdf->win_ptr, -100, 20, 0xffd700, SWITCH);
	mlx_string_put(fdf->mlx_ptr, fdf->win_ptr, -100, 30, 0xffd700, COLORS);
	mlx_string_put(fdf->mlx_ptr, fdf->win_ptr, -100, 40, 0xffd700, Z);	
}


